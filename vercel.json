{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "post.dmrt.ie"
        }
      ],
      "destination": "https://patcher.dmrt.ie/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(apple-touch-icon|apple-icon|icon|favicon|android-icon|ms-icon).*\\.(png|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}

