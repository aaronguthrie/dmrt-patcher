{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "post.dmrt.ie"
        }
      ],
      "destination": "https://patcher.dmrt.ie/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/"
    }
  ]
}

